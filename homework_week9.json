{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "Australia Energy Consumption",
    "projection": {
      "type": "equalEarth"
    },
    "layer": [
      {
        "data": {
          "url": "au-states-topo.json",
          "format": {
            "type": "topojson",
            "feature": "ne_110m_graticules_30"
          }
        },
        "mark": {
          "type": "geoshape",
          "fill": "lightgray",
          "stroke": "white"
        }
      },
      {
        "data": {
          "url": "https://tngu0367.github.io/FIT3179/test_data.csv"
        },
        "mark": {
          "type": "circle",
          "tooltip": {
            "content": "data"
          }
        },
        "encoding": {
          "longitude": {
            "field": "Longitude",
            "type": "quantitative"
          },
          "latitude": {
            "field": "Latitude",
            "type": "quantitative"
          },
          "size": {
            "field": "Consumption",
            "type": "quantitative"
          },
          "color": {
            "field": "State",
            "type": "nominal"
          }
        }
      }
    ]
  }
  